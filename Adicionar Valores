
def add_values():
    conexao = pymysql.connect(
        host = 'localhost',
        user = 'root',
        passwd = ''
    )
    cursor = conexao.cursor()
    database = 'faculdade'
    table = 'caneca'
    cursor.execute(f'DESCRIBE {database}.{table}')
    tabela_described = cursor.fetchall()
    colunas = []
    valores = []
    for i in tabela_described:
        if i[5] != 'auto_increment':
            valor = input(f"""Coluna: {i[0]}
Tipo: {i[1]}
Padr√£o {i[4]}
Adicionar: """)
            colunas.append(i[0])
            valores.append(valor)
    colunas, valores = tuple(colunas), tuple(valores)
    comando = f'INSERT INTO {database}.{table} ('
    for item in colunas:
        if item == colunas[len(colunas) - 1]:
            comando += item + f') VALUES {valores}'
        else:
            comando += item + ', '
    cursor.execute(comando)
    conexao.commit()
    print(cursor.rownumber)
